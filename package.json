{
  "name": "vesting-coin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "SERVICES_ENV=${SERVICES_ENV:-development} next dev",
    "dev:build": "SERVICES_ENV=${SERVICES_ENV:-development} next build && next export -o webdist/",
    "dev:start": "SERVICES_ENV=${SERVICES_ENV:-development} next start",
    "stage": "SERVICES_ENV=${SERVICES_ENV:-staging} next dev",
    "stage:build": "SERVICES_ENV=${SERVICES_ENV:-staging} next build",
    "stage:start": "SERVICES_ENV=${SERVICES_ENV:-staging} next start",
    "prod": "SERVICES_ENV=${SERVICES_ENV:-production} next dev",
    "prod:build": "SERVICES_ENV=${SERVICES_ENV:-production} next build",
    "prod:start": "SERVICES_ENV=${SERVICES_ENV:-production} next start",
    "lint": "next lint",
    "build:next": "next build && next export -o webdist/",
    "start": "next start"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.3",
    "@metamask/providers": "^9.0.0",
    "@reduxjs/toolkit": "^1.8.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@web3-react/walletlink-connector": "^6.2.14",
    "bootstrap": "^5.1.3",
    "dot": "^1.1.3",
    "dotenv": "^16.0.1",
    "env": "^0.0.2",
    "ethers": "^5.5.4",
    "moment": "^2.29.4",
    "next": "12.1.5",
    "node-sass": "^7.0.1",
    "react": "18.1.0",
    "react-bootstrap": "^1.5.0",
    "react-countdown": "^2.3.2",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.2",
    "react-toastify": "^7.0.3",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "reduxjs-toolkit-persist": "^7.2.1",
    "sass": "^1.51.0",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.6.4"
  }
}
